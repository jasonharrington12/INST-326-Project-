        "\n",
        "\n",
        "def get_highest_rated(library):\n",
        "    \"\"\"Finds and displays the highest rated book(s).\"\"\"\n",
        "    rated_books = []\n",
        "\n",
        "    # Find all rated books\n",
        "    for book in library:\n",
        "        if book[\"rating\"] is not None:\n",
        "            rated_books.append(book)\n",
        "\n",
        "    # Find highest rating\n",
        "    if rated_books:\n",
        "        max_rating = max(book[\"rating\"] for book in rated_books)\n",
        "        top_books = [book for book in rated_books if book[\"rating\"] == max_rating]\n",
        "\n",
        "        print(f\"\\nHighest rated book(s) with {max_rating}/5:\")\n",
        "        for book in top_books:\n",
        "            print(f\"- {book['title']}\")\n",
        "    else:\n",
        "        print(\"\\nNo books have been rated yet!\")\n",
        "\n",
        "def get_unrated_books(library):\n",
        "    \"\"\"Displays all books that haven't been rated yet.\"\"\"\n",
        "    unrated = []\n",
        "\n",
        "    # Find unrated books\n",
        "    for book in library:\n",
        "        if book[\"rating\"] is None:\n",
        "            unrated.append(book)\n",
        "\n",
        "    # Display results\n",
        "    if unrated:\n",
        "        print(f\"\\nUnrated books ({len(unrated)}):\")\n",
        "        for book in unrated:\n",
        "            print(f\"- {book['title']}\")\n",
        "    else:\n",
        "        print(\"\\nAll books have been rated!\")\n",
        "\n",
        "def get_highest_rated_movie(movies):\n",
        "    \"\"\"Finds and displays the highest rated movie(s).\"\"\"\n",
        "    rated_movies = []\n",
        "\n",
        "    # Find all rated movies\n",
        "    for movie in movies:\n",
        "        if movie[\"rating\"] is not None:\n",
        "            rated_movies.append(movie)\n",
        "\n",
        "    # Find highest rating\n",
        "    if rated_movies:\n",
        "        max_rating = max(movie[\"rating\"] for movie in rated_movies)\n",
        "        top_movies = [movie for movie in rated_movies if movie[\"rating\"] == max_rating]\n",
        "\n",
        "        print(f\"\\nHighest rated movie(s) with {max_rating}/5:\")\n",
        "        for movie in top_movies:\n",
        "            print(f\"- {movie['title']}\")\n",
        "    else:\n",
        "        print(\"\\nNo movies have been rated yet!\")\n",
        "\n",
        "\n",
        "def get_unrated_movies(movies):\n",
        "    \"\"\"Displays all movies that haven't been rated yet.\"\"\"\n",
        "    unrated = []\n",
        "\n",
        "    # Find unrated movies\n",
        "    for movie in movies:\n",
        "        if movie[\"rating\"] is None:\n",
        "            unrated.append(movie)\n",
        "\n",
        "    # Display results\n",
        "    if unrated:\n",
        "        print(f\"\\nUnrated movies ({len(unrated)}):\")\n",
        "        for movie in unrated:\n",
        "            print(f\"- {movie['title']}\")\n",
        "    else:\n",
        "        print(\"\\nAll movies have been rated!\")\n",
        "\n",
        "\n",
        "\n",
        "def get_top_users(users):\n",
        "    \"\"\"Finds and displays users who have consumed the most books and movies combined.\"\"\"\n",
        "    print(\"\\n=== Top Library Users ===\")\n",
        "\n",
        "    # Calculate total items consumed for each user\n",
        "    user_stats = []\n",
        "    for user in users:\n",
        "        books_read = user.get(\"books_read\", 0)\n",
        "        movies_watched = user.get(\"movies_watched\", 0)\n",
        "        total_items = books_read + movies_watched\n",
        "\n",
        "        user_stats.append({\n",
        "            \"name\": user[\"name\"],\n",
        "            \"books\": books_read,\n",
        "            \"movies\": movies_watched,\n",
        "            \"total\": total_items\n",
        "        })\n",
        "\n",
        "    # Sort users by total items consumed (highest first)\n",
        "    user_stats.sort(key=lambda x: x[\"total\"], reverse=True)\n",
        "    #Used ai to generate this as I couldnt figure it out\n",
        "\n",
        "    # Check if any users have consumed items\n",
        "    if not user_stats or user_stats[0][\"total\"] == 0:\n",
        "        print(\"\\nNo users have read books or watched movies yet!\")\n",
        "        return\n",
        "\n",
        "    # Display top users\n",
        "    print(\"\\nTop users by total books and movies consumed:\\n\")\n",
        "\n",
        "    rank = 1\n",
        "    prev_total = None\n",
        "\n",
        "    for i, user in enumerate(user_stats):\n",
        "        # Skip users with 0 items\n",
        "        if user[\"total\"] == 0:\n",
        "            break\n",
        "\n",
        "        # Update rank only if total changed (handles ties)\n",
        "        if prev_total is None or user[\"total\"] != prev_total:\n",
        "            rank = i + 1\n",
        "\n",
        "        print(f\"{rank}. {user['name']}\")\n",
        "        print(f\"   Books read: {user['books']}\")\n",
        "        print(f\"   Movies watched: {user['movies']}\")\n",
        "        print(f\"   Total: {user['total']}\")\n",
        "        print()\n",
        "\n",
        "        prev_total = user[\"total\"]\n",
        "\n",
        "    # Show summary statistics\n",
        "    total_books = sum(u[\"books\"] for u in user_stats)\n",
        "    total_movies = sum(u[\"movies\"] for u in user_stats)\n",
        "    active_users = sum(1 for u in user_stats if u[\"total\"] > 0)\n",
        "\n",
        "    print(f\"--- Summary ---\")\n",
        "    print(f\"Active users: {active_users}\")\n",
        "    print(f\"Total books read: {total_books}\")\n",
        "    print(f\"Total movies watched: {total_movies}\")\n",
        "    print(f\"Total items consumed: {total_books + total_movies}\")"
      ]
